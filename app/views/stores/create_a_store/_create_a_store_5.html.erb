<div style="height: 8px; top: 71px;" class="progress rounded-0">
  <div role="progressbar" style="width: 100%" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100" class="progress-bar"></div>
</div>
<section class="py-5 py-lg-6">
  <div class="container text-center">
    <p class="subtitle text-primary"><%= t('.add_new_listing') %></p>
    <h1 class="h2 mb-5"><%= t('.listing_uploaded') %></h1>
    <p class="mb-5"><%= image_tag('celebration.svg', style: "width: 400px;", class: "img-fluid") %></p>
    <p class="text-muted mb-5"><%= t('.thank_you') %>, <%= current_user.name || current_user.email %>. <%= t('.reviewed_shortly') %></p>
    <p class="mb-5"><%= link_to(t('.see_your_listing'), store_path(current_user.stores.last), class: "btn btn-primary mr-2 mb-2") %>
  </div>
</section>