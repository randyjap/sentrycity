<!-- Gallery-->
<h3 class="mb-4">Gallery</h3>
<div class="row ml-n1 mr-n1">
  <% store.photos.each do |photo| %>
    <div class="col-lg-4 col-6 px-1 mb-2">
      <%= link_to image_tag(photo, class: "img-fluid"), photo_upload_path(photo.id), method: :delete, remote: true, data: {confirm: "Would you like to delete this photo?"} %>
    </div>
  <% end %>
</div>